{
  "name": "u5-r2-query",
  "version": "0.0.3-0",
  "description": "Query an API (GraphQL, maybe others?) in a React/Redux application",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --source-maps --ignore \"**/__tests__/*.js\"",
    "prepublish": "npm run clean && npm test && npm run build",
    "clean": "rimraf dist",
    "test": "flow && jest",
    "test:watch": "npm test -- --watch",
    "sample:basic": "webpack-dev-server --config ./sample/webpack.config.js",
    "sample:rerendering": "webpack-dev-server --config ./sample-rerendering/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.uber5.com:chris/u5-r2-query.git"
  },
  "keywords": [
    "react",
    "redux",
    "query",
    "graphql",
    "uber5"
  ],
  "author": "Chris Oloff <chris@uber5.com> (http://about.uber5.com/)",
  "license": "ISC",
  "dependencies": {
    "ramda": ">=0.19 <1.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-redux": "^4.0.0 || ^5.0.0",
    "redux": "^2.0.0 || ^3.0.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "redux": "^2.0.0 || ^3.0.0",
    "react-redux": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "jest": "18.1.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-redux": "^4.0.0 || ^5.0.0",
    "react-test-renderer": "15.4.2",
    "redux": "^2.0.0 || ^3.0.0",
    "redux-mock-store": "1.2.1",
    "redux-thunk": "2.2.0",
    "rimraf": "2.5.4",
    "source-map-support": "0.4.10",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "jest": {
    "rootDir": "src",
    "setupTestFrameworkScriptFile": "../test/setup.js"
  }
}
